<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.electricalmeter">

    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/watthour"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/watthour"
        android:supportsRtl="true"
        android:theme="@style/Theme.ElectricalMeter">
        <activity android:name=".CameraActivity"/>
        <activity android:name=".WiringInfo" />
        <activity android:name=".CalculationWiring" />
        <activity android:name=".VoltAmperScreen" />
        <activity android:name=".InfoScreen" />
        <activity android:name=".powerscreen" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>